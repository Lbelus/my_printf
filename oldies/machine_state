


#define STATE_NORMAL 0
#define STATE_INT 0


int main(int argc, char** argv){

    int i = 0;
    for(int y = 2; y < argc; y++){
        i++;
        if(argv[i][0] == '-'){
            for(int x = 1; argv[i][x] != '\0'; x++ ){
                char flag = argv[i][x];
                switch(flag){
                case 't':
                break;
                case 'c': 
                break;
                }
            }            
        }
    }


}